
<!DOCTYPE html>

<html>
<head>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.js"></script>
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>

    <meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible"content="IE=edge">
<meta name="viewport" content="width=device-width,initial-scale=1.0">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css"integrity="sha384-KyZXEAg30hqLMpGBr+8fhAXLRK2vvoC2f3809zVXn8CASQIVFZ033BCs2P8p/We"crossorigin="anonymous"> 
<link rel="stylesheet" href="index.css">

   
    
</head>
<body>
  
    <div class="main-container"> 
         <header class="header-section">
            <div class="logo-user">
               <center> <img class="logo" src="./assets/logo.png" alt="logo"></img></center>
                <a  class="button2" id="table" href="/articletable">See Table View</a>
                <a class="button2" href="/register">Add New User</a>
                <a class="button2" href="/articlechart">Chart</a>
            </div>
        </header>


    <section>
          
        <h1 class="total">Total Article:<%= totalCount %></h1>
      
        
        
        
      <% items.forEach(items => { %> 
       
             <div class="dish-card">
                <div class="inside">
                <div class="dish-name"><%= items.paperTitle %></div>
                <div class="description"><%= items.authorName %></div>
                <div class="description"><%= items.Department %></div>
                <div class="dish-name"><%= items.journalName %>
                   
                </div>
                <div class="line"></div>
                <div class="rating" >Year <%= items.year %></div>
                <div class="rating" >ISSN Number <%= items.ISSNNumber %></div>
   
                
        
            
         

                   <!-- <center><a class="file" href="/view/<%= items.data %>" target="_blank" download>Download PDF</a></center>  -->
                   
                   <center><a class="file" id="down" href="<%= items.data%>" target="_blank" >View <Article></Article></a></center> 
            
                </div>
                
          
        
    </br></br></br></br>
</div>
<% }); %>
</section>
</div>
  

</body>
</html>
